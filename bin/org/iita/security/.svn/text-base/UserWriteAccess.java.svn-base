package org.iita.security;

import org.iita.security.model.User;

/**
 * @author mobreza
 */
public interface UserWriteAccess extends UserAccess {
	/**
	 * Object level "write" check. The method should return <code>true</code> if the user can persist/merge the entity.
	 * 
	 * @param user
	 * @return
	 */
	boolean hasWriteAccess(User user);
}
